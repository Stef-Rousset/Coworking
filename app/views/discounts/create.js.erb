if (document.querySelector('#custom-modal-<%= @discount.office_id %>') != null) {
  document.querySelector('.custom-modal-body-<%= @discount.office_id %>').remove();
  document.querySelector('#custom-modal-<%= @discount.office_id %>-label')
  .insertAdjacentHTML('afterend',"<%= j render partial: 'offices/custom-modal-body', locals: { office: @discount.office } %>");
}
else {
  document.querySelector('.office-card__title<%= @discount.office_id %>')
  .insertAdjacentHTML('afterend', "<%= j render partial: 'offices/custom-modal', locals: { office: @discount.office } %>");
}

